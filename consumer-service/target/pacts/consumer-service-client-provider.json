              ],
              "combine": "AND"
            },
            "$.phoneNumber": {
              "matchers": [
                {
                  "match": "integer"
                }
              ],
              "combine": "AND"
            }
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "user": {
            "name": "ytPCJ",
            "lastName": "xlEaT",
            "address": "Street OxpuKA",
            "age": 44,
            "phoneNumber": 985201621
          },
          "activated": true
        }
      },
