      "description": "a request to create a new client to unsubscribe",
      "request": {
        "method": "POST",
        "path": "/client-provider/client",
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "lastName": "xlEaT",
          "address": "Street OxpuKA",
          "phoneNumber": 985201621,
          "name": "ytPCJ",
          "age": 44
        },
        "matchingRules": {
          "body": {
            "$.name": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$.lastName": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
