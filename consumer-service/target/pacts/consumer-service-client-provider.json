    {
      "description": "a request to notify all active clients",
      "request": {
        "method": "GET",
        "path": "/client-provider/clients"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "clients": [
            {
              "user": {
                "lastName": "string",
                "address": "string",
                "phoneNumber": 100,
                "name": "string",
                "age": 100
              },
              "activated": true
            }
          ]
        },
        "matchingRules": {
          "body": {
            "$.clients": {
              "matchers": [
                {
                  "match": "type",
                  "min": 1
                }
              ],
              "combine": "AND"
            },
            "$.clients[*].activated": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$.clients[*].user.name": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$.clients[*].user.lastName": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$.clients[*].user.address": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$.clients[*].user.age": {
              "matchers": [
                {
                  "match": "integer"
                }
              ],
              "combine": "AND"
            },
            "$.clients[*].user.phoneNumber": {
              "matchers": [
                {
                  "match": "integer"
                }
              ],
              "combine": "AND"
            }
          }
        },
        "generators": {
          "body": {
            "$.clients[*].user.name": {
              "type": "RandomString",
              "size": 20
            },
            "$.clients[*].user.lastName": {
              "type": "RandomString",
              "size": 20
            },
            "$.clients[*].user.address": {
              "type": "RandomString",
              "size": 20
            },
            "$.clients[*].user.age": {
              "type": "RandomInt",
              "min": 0,
              "max": 2147483647
            },
            "$.clients[*].user.phoneNumber": {
              "type": "RandomInt",
              "min": 0,
              "max": 2147483647
            }
          }
        }
      },
